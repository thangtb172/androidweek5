<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.week3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Week3">
        <activity
            android:name="ui.auth.RestaurantListActivity"
            android:label="@string/title_activity_restaurant_list"
            android:theme="@style/Theme.Week3.NoActionBar"></activity>
        <activity android:name="ui.auth.ProfileActivity" />
        <activity android:name="ui.auth.LoginActivity" />
        <activity android:name="ui.auth.SignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>